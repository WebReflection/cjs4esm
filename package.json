  "bugs": {
    "url": "https://github.com/WebReflection/cjs4esm/issues"
  },
  "homepage": "https://github.com/WebReflection/cjs4esm#readme",
  "devDependencies": {
    "circular-json": "^0.5.1",
    "uglify-es": "^3.2.2"
  }
}
